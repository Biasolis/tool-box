# Configuração do Banco de Dados PostgreSQL
DB_USER=toolboxuser
DB_PASSWORD=toolboxpassword
DB_NAME=toolboxdb

# Chave secreta para gerar tokens JWT
JWT_SECRET=changemeinproductionwithalongrandomstring

# Credenciais do Google Cloud Storage (S3)
# Crie uma chave HMAC no painel do GCP para uma conta de serviço
S3_ENDPOINT=https://storage.googleapis.com
S3_REGION=auto
S3_BUCKET_NAME=your-gcp-bucket-name
S3_ACCESS_KEY=your-gcp-hmac-access-key
S3_SECRET_KEY=your-gcp-hmac-secret-key
S3_FORCE_PATH_STYLE=false